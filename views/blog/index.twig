<div data-page-type="listing">

	<div class="content_block" data-listing-title>
		<div class="breadcrumbs">
			{% for link,name in seo.breadcrumbs %}
				<a class="breadcrumbs_item" href="{{link}}">{{name}}</a>
			{% endfor %}
		</div>
		<div class="title">
			<h1>{{seo.h1}}</h1>
		</div>
	</div>

	<div class="object_gallery_wrap">
        <div class="listing_widget_arrow _prev"></div>
        <div class="listing_widget_arrow _next"></div>
        <div class="object_gallery_container swiper-container" data-other-objects-swiper>
            <div class="object_gallery_swiper swiper-wrapper" data-gallery-list>
                {% for image in title_item.restaurant_images%}
                    <div class="object swiper-slide">
                       <img src="{{ image.realpath }}" data-gallery-img-view/>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>

	<div class="items_list_listing content_block" data-listing-wrapper>
		<div class="items_list" data-listing-list>

			{{ this.render('//components/generic/listing_blog.twig', {'items':items, 'img_alt':seo.img_alt}) | raw }}
			

		</div>
	</div>

	<div class="content_block"> 
		<div data-pagination-wrapper>
			{{pagination | raw }}
		</div>
		<div data-append-items>Показать еще</div>
	</div>


</div>
